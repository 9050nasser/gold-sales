 "html": "<!DOCTYPE html>\r\n<html>\r\n<head>\r\n  <style>\r\n    @page {\r\n      size: A4 portrait;\r\n      margin: 0;\r\n      \r\n    }\r\n    \r\n    body {\r\n      font-family: Arial, sans-serif;\r\n      margin: 20px;\r\n      color: black;\r\n    }\r\n    \r\n    table {\r\n      width: 100%;\r\n      border-collapse: collapse;\r\n      margin-bottom: 20px;\r\n      font-weight: bold;\r\n      text-align: center;\r\n      color: black;\r\n    }\r\n    \r\n    th, td {\r\n      border: 1px solid black;\r\n      padding: 5px;\r\n      font-weight: bold;\r\n      text-align: center;\r\n      color: black;\r\n    }\r\n    \r\n    tr, th {\r\n        color: black;\r\n    }\r\n    \r\n    .center {\r\n      text-align: center;\r\n      color: black;\r\n    }\r\n    \r\n    .right {\r\n      text-align: center;\r\n      color: black;\r\n    }\r\n    \r\n    .bold {\r\n      font-weight: bold;\r\n      color: black;\r\n    }\r\n    \r\n    /* Adjustments for the second table */\r\n    .item-table {\r\n      font-size: 12px;\r\n      font-weight: bold;\r\n      color: black;\r\n    }\r\n    \r\n    .item-table th, .item-table td {\r\n      padding: 3px;\r\n      font-weight: bold;\r\n      color: black;\r\n    }\r\n  </style>\r\n</head>\r\n<body>\r\n  \r\n  <table>\r\n    <tr>\r\n      <th>\u0631\u0642\u0645 \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629</th>\r\n      <th>{{ doc.name }}</th>\r\n      <th>\u0627\u0644\u0633\u062c\u0644 \u0627\u0644\u062a\u062c\u0627\u0631\u064a</th>\r\n      <th>1010464821</th>\r\n    </tr>\r\n    <tr>\r\n      <td>\u062a\u0627\u0631\u064a\u062e \u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0629</td>\r\n      <td>{{ doc.posting_date }}</td>\r\n      <td>\u0631\u062e\u0635\u0629 \u0645\u0639\u0627\u062f\u0646</td>\r\n      <td>12/10100420</td>\r\n    </tr>\r\n    <tr>\r\n      <td>\u0627\u0644\u0628\u0627\u0626\u0639</td>\r\n      <td class=\"bold\">{{ doc.owner }}</td>\r\n      <td>\u0627\u0644\u0641\u0631\u0639</td>\r\n      <td>\u0645\u0635\u0646\u0639 \u062f\u0627\u0646\u0647 \u0627\u0644\u0634\u0631\u0642 \u0644\u0635\u064a\u0627\u063a\u0629 \u0627\u0644\u0630\u0647\u0628 \u0648\u0627\u0644\u0645\u062c\u0648\u0647\u0631\u0627\u062a</td>\r\n    </tr>\r\n    <tr>\r\n      <td>\u0627\u0644\u0639\u0645\u064a\u0644</td>\r\n      <td class=\"bold\">{{ doc.customer }}</td>\r\n      <td>\u0627\u0644\u0631\u0642\u0645 \u0627\u0644\u0636\u0631\u064a\u0628\u064a \u0644\u0644\u0639\u0645\u064a\u0644</td>\r\n      <td>{{ doc.tax_id }}</td>\r\n    </tr>\r\n  </table>\r\n  <table class=\"item-table\">\r\n    <tr>\r\n      <th>\u0631\u0642\u0645 \u0627\u0644\u0635\u0646\u0641</th>\r\n      <th>\u0627\u0633\u0645 \u0627\u0644\u0635\u0646\u0641</th>\r\n      <th>\u0627\u0644\u0643\u0645\u064a\u0629</th>\r\n      <th>\u0648\u0632\u0646 \u0645\u0627 \u062e\u0644\u0649 \u0645\u0646 \u0627\u0644\u0645\u0639\u062f\u0646</th>\r\n      <th>\u0627\u0644\u0639\u064a\u0627\u0631</th>\r\n      <th>\u0627\u062c\u0631\u0629</th>\r\n      <th>\u0627\u0644\u0648\u0632\u0646</th>\r\n      <th>\u0645\u0639\u062f\u0646</th>\r\n      <th>\u0627\u0644\u0645\u0628\u0644\u063a \u0642\u0628\u0644 \u0627\u0644\u0636\u0631\u064a\u0628\u0629</th>\r\n      <th>\u0627\u0644\u0636\u0631\u064a\u0628\u0629</th>\r\n      <th>\u0627\u0644\u0645\u0628\u0644\u063a \u0628\u0639\u062f \u0627\u0644\u0636\u0631\u064a\u0628\u0629</th>\r\n    </tr>\r\n    {% for item in doc.items %}\r\n    {% set aftertax = item.amount * 1.15 %}\r\n    <tr>\r\n      <td>{{ item.item_code }}</td>\r\n     <td>{{ item.item_name }}</td>\r\n      <td>{{ item.qty }}</td>\r\n      <td>{{ item.\u0648\u0632\u0646_\u0645\u0627_\u062e\u0644\u064a_\u0645\u0646_\u0627\u0644\u0645\u0639\u062f\u0646 }}</td>\r\n      <td>{{ item.\u0627\u0644\u0639\u064a\u0627\u0631 }}</td>\r\n      <td>{{ item.rate }}</td>\r\n      <td>{{ item.qty }}</td>\r\n      <td>{{ item.\u0627\u0644\u0645\u0639\u062f\u0646 }}</td>\r\n      <td>{{ item.amount|float }}</td>\r\n      <td>{{ item.amount * 0.15|float }}</td>\r\n      <td>{{ aftertax|round(2) }}</td>\r\n    </tr>\r\n    {% endfor %}\r\n    <tr>\r\n      <td colspan=\"6\" rowspan=\"1\" class=\"bold\">\u0627\u0644\u0625\u062c\u0645\u0627\u0644\u064a</td>\r\n      <td>{{ doc.total_qty }}</td>\r\n      <td></td>\r\n      <td>{{ doc.total }}</td>\r\n      <td>{{ doc.total_taxes_and_charges }}</td>\r\n      <td>{{ doc.grand_total }}</td>\r\n    </tr>\r\n  </table>\r\n  \r\n</body>\r\n</html>\r\n",
